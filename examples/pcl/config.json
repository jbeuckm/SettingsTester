{
  "testing": {
    "test_size": 30,
    "population": 10,
    "generators": 3,
    "wildcards": 2
  },

  "command": "../../../pcl/build/correspondence_grouping",

  "arguments": {

    "model": {
      "test_set": true,
      "type": "path",
      "path": "/myshare/pointclouds/objects/*.pcd"
    },
    "scene": {
      "test_set": true,
      "type": "path",
      "path": "/myshare/pointclouds/scenes/*.pcd"
    },

"algo": {
"type": "option",
"values": ["Hough","GC"],
"prefix": "--algorithm"
},

    "autores": {
      "type": "option",
      "values": ["-r"]
    },
    "model_ss": {
      "type": "float",
      "prefix": "--model_ss",
      "range": [6,9]
    },
    "scene_ss": {
      "prefix": "--scene_ss",
      "type": "float",
      "range": [15,25]
    },
    "rf_rad": {
      "prefix": "--rf_rad",
      "type": "option",
      "values": [5,10,15,20]
    },
    "descr_rad": {
      "prefix": "--descr_rad",
      "type": "option",
      "values": [5,10,15,20]
    },
    "cg_size": {
      "prefix": "--cg_size",
      "type": "integer",
      "range": [8, 12]
    }
  }
}
