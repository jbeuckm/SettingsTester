{
  "testing": {
    "test_size": 30,
    "population": 10,
    "generators": 3,
    "wildcards": 2
  },

  "command": "../../../pcl/build/correspondence_grouping",

  "arguments": {

    "model": {
      "test_set": true,
      "type": "path",
      "path": "/myshare/pointclouds/objects/*.pcd"
    },
    "scene": {
      "test_set": true,
      "type": "path",
      "path": "/myshare/pointclouds/scenes/*.pcd"
    },

    "autores": {
      "type": "option",
      "values": ["-r"]
    },
    "model_ss": {
      "type": "float",
      "prefix": "--model_ss",
      "range": [7.3, 7.7]

    },
    "scene_ss": {
      "prefix": "--scene_ss",
      "type": "integer",
      "range": [17, 23]
    },
    "rf_rad": {
      "prefix": "--rf_rad",
      "type": "integer",
      "range": [8, 12]
    },
    "descr_rad": {
      "prefix": "--descr_rad",
      "type": "integer",
      "range": [13, 17]
    },
    "cg_size": {
      "prefix": "--cg_size",
      "type": "integer",
      "range": [8, 12]
    }
  }
}
