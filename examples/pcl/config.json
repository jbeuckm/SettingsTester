{
  "testing": {
    "test_size": 10,
    "population": 8,
    "generators": 3
  },

  "command": "../../../pcl/build/correspondence_grouping",

  "arguments": {

    "model": {
      "test_set": true,
      "type": "path",
      "path": "/myshare/pointclouds/objects/*.pcd"
    },
    "scene": {
      "test_set": true,
      "type": "path",
      "path": "/myshare/pointclouds/scenes/*.pcd"
    },

    "autores": {
      "type": "option",
      "values": [null, "-r"]
    },
    "model_ss": {
      "type": "float",
      "prefix": "--model_ss",
      "range": [7.0, 8.0]

    },
    "scene_ss": {
      "prefix": "--scene_ss",
      "type": "integer",
      "range": [15, 25]
    },
    "rf_rad": {
      "prefix": "--rf_rad",
      "type": "integer",
      "range": [5, 15]
    },
    "descr_rad": {
      "prefix": "--descr_rad",
      "type": "integer",
      "range": [10, 20]
    },
    "cg_size": {
      "prefix": "--cg_size",
      "type": "integer",
      "range": [5, 15]
    }
  }
}
